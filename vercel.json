{
  "functions": {
    "api/*.py": { "runtime": "vercel-python@3.9.0" }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/app.py" },
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/:path((?!api).*)", "destination": "/frontend/index.html" }
  ]
}
